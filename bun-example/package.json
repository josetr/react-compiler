{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "build": "./build.",
    "build-no-plugins": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --production --env='BUN_PUBLIC_*' --plugin=./bun-plugin-react-compiler-dev.ts ",
    "serve": "bun run build && bunx serve dist",
    "start": "NODE_ENV=production bun src/index.ts"
  },
  "dependencies": {
    "@babel/core": "^7.28.5",
    "react": "^19",
    "babel-plugin-react-compiler": "^1.0.0",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
  }
}